{
  "name": "Attacker Chrome Extension",
  "version": "1.0",
  "description": "Attacker Chrome Extension POC by ProCheckUp",
  "permissions": ["activeTab", "tabs", "https://*/*", "http://*/*", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action":
	{
	"default_icon": "images/pcu32.png",
	"default_popup": "popup.html"
	},
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/pcu32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
